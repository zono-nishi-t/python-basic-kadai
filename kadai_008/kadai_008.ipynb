        "import random\n",
        "\n",
        "# ランダムな正の整数を生成\n",
        "var = random.randint(1, 100)\n",
        "\n",
        "# 条件判定\n",
        "if var % 3 == 0 and var % 5 == 0:\n",
        "    print(\"FizzBuzz\")\n",
        "elif var % 3 == 0:\n",
        "    print(\"Fizz\")\n",
        "elif var % 5 == 0:\n",
        "    print(\"Buzz\")\n",
        "else:\n",
        "    print(var)\n"
      ]
    }
  ]
